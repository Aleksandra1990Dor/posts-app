{
	"singleQuote": true,
	"trailingComma": "none",
	"arrowParens": "avoid",
	"useTabs": true,
	"tabWidth": 2,
	"semi": false,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^@/core/(.*)$",
		"^@/core/components/(.*)$",
		"^@/components(.*)$",
		"^@/components/layout/(.*)$",
		"^@/components/screens/(.*)$",
		"^@/components/ui/(.*)$",
		"^@/styles(.*)$",
		"^@/config(.*)$",
		"^@/utils(.*)$",
		"^@/services(.*)$",
		"^@/store(.*)$",
		"^@/api(.*)$",
		"^../(.*)",
		"^./(.*)"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
